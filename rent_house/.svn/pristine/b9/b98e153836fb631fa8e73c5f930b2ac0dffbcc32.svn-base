body{
    margin-left: 0px;
    margin-right: 0px;
}
#big{
    height: 650px;
    width: 100%;
    margin-top: 90px;
    background-color: #555555;
    background-image: url(resource/images/login_bg.jpg);
}
#login{
    height: 600px;
    width: 500px;
    border: 1px solid white;
    margin: 0px auto;
    position: relative;
    top: 25px;
    background-color: white;
    border-radius: 15px;
    /* font-family: 楷体; */
}
#title{
    height: 70px;
}
h1{
    font-size: 50px;
    font-family: 华文新魏;
}
.inp{
    width: 230px;
    height: 30px;
    border-radius: 8px;
}

.input{
    font-size: 25px;
    /* border: 1px solid black; */
    text-align: center;
    margin-top: 35px;
    height: 45px;
    /* border: 1px solid black; */
}
input{
    font-size: 20px;
    border: 1px solid black;
}
#btn{
    width: 150px;
    height: 50px;
    cursor: pointer;
    margin-top: 10px;
    background-color: white;
}

#our{
    position: absolute;
    top: 25px;
    left: 500px;
    font-size: 50px;
    font-family: 华文新魏;
    color: crimson;
}
.sp{
    font-size: 10px;
    position: absolute;
    left: 175px;
}
table{
    margin: 0 auto;
}
.td1{
    text-align: right;
    height: 50px;
}
a{
    text-decoration: none;
    float: left;
    position: relative;
    bottom: 20px;
    left: 10px;
    color: black;
}
a:hover{
    color: red;
}